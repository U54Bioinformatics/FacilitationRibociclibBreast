# Create Supplmentary Figure 1a, b (CAMA1 cells)
Run suppfigsc.R to create CAMAfits.pdf
This reads three files:
  cdat <- read.csv("../DATA/fitting_equations.csv")
  areas <- read.csv(paste0("../DATA/",filenm,"_area.csv"))
  fluor <- read.csv(paste0("../DATA/",filenm,"_fluor.csv"))

# Create Supplmentary Figure 1c, d (MCF7LY2 cells)
Run suppfigsl.R to create LY2fits.pdf
This reads four files:
  LYraw <- read.csv("../DATA/coculture.csv",header=TRUE)
  Areasc <- read.csv("../DATA/LYarea.csv",header=FALSE)
  Sfluorsc <- read.csv("../DATA/LYSfluor.csv",header=FALSE)
  Rfluorsc <- read.csv("../DATA/LYRfluor.csv",header=FALSE)

# Create Supplmentary Figure 1e, f (MCF7 cells)
Run suppfigsl.R to create MCF7fits.pdf
This reads five files:
  gmn <- read.csv("../DATA/growth_curves_feb25_2021.csv",header=TRUE)
  Areasm <- read.csv("../DATA/AreanewMCF7.csv",header=FALSE)
  Fractionsm <- read.csv("../DATA/FractionnewMCF7.csv",header=FALSE)
  Sfluorsm <- read.csv("../DATA/MCF7_Sfluor.csv",header=FALSE)
  Rfluorsm <- read.csv("../DATA/MCF7_Rfluor.csv",header=FALSE)

